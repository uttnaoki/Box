main(){
  define matrix1[2][2];
  define matrix2[2][2];
  define matrix3[2][2];

  define i;
  define l;
  define k;

  matrix1[0][0] = 1;
  matrix1[0][1] = 2;
  matrix1[1][0] = 3;
  matrix1[1][1] = 4;

  matrix2[0][0] = 5;
  matrix2[0][1] = 6;
  matrix2[1][0] = 7;
  matrix2[1][1] = 8;

  for(i=0;i<2;i++){
    for(l=0;l<2;l++){
      for(k=0;k<2;k++){
	matrix3[i][l] = matrix3[i][l] + matrix1[i][k] * matrix2[k][l];
      }
    }
  }

}
